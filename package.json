{
  "name": "m-learning-nest-api",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main.js",
    "start:dev": "cross-env NODE_ENV=test nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=test nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --verbose --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "node dist/database/seeder",
    "seed:refresh": "node dist/database/seeder --refresh",
    "clear_jest": "jest --clearCache",
    "eslint-check": "eslint --no-error-on-unmatched-pattern src/**/*.ts"
  },
  "pre-commit": [
    "eslint-check"
  ],
  "dependencies": {
    "@anchan828/nest-sendgrid": "^0.6.34",
    "@casl/ability": "^6.3.3",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.4.3",
    "@nestjs/platform-ws": "^9.4.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.4.3",
    "@sendgrid/mail": "^7.7.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "digitalocean-spaces": "^1.0.0",
    "dotenv": "^16.0.3",
    "express-ctx": "^0.1.1",
