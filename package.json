    "mongoose": "^6.10.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nestjs-seeder": "^0.3.0",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.21",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "password-hash": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.0",
